{
  "name": "togglebar",
  "displayName": "ToggleBar",
  "description": "Add status bar buttons to toggle primary and secondary sidebars",
  "version": "0.0.1",
  "publisher": "togglebar-publisher",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "togglebar.togglePrimarySidebar",
        "title": "Toggle Primary Sidebar"
      },
      {
        "command": "togglebar.toggleSecondarySidebar",
        "title": "Toggle Secondary Sidebar"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  }
}
